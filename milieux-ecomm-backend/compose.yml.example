# Use this file in case you want to use docker for postgresql

version: '3'
services:
  postgres:
    image: 'postgres:latest'
    environment:
      - 'POSTGRES_DB=YOUR_DATABASE_NAME
      - 'POSTGRES_PASSWORD=YOUR_DATABASE_PASSWORD
      - 'POSTGRES_USER=YOUR_DATABASE_USERNAME
    ports:
      - POSTGRES_PORT:5432
